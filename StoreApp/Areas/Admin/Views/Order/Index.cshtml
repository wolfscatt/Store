@model IEnumerable<Order>

<div class="text-center my-5">
    <div class="display-6">
        Orders
    </div>
</div>
<hr>
<div class="my-5">
    @if (Model.Count() > 0)
    {
        @foreach (var order in Model)
        {
            <div class="my-3">
                <partial name="_OrderDetails" model="@order" />
            </div>
        }
    }
    else
    {
        <div class="alert alert-info">
            <p class="lead">There is no order yet.</p>
        </div>
    }
</div>